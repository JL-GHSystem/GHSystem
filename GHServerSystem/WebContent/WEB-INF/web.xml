<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>GHServerSystem</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>initServelt</servlet-name>
    <servlet-class>com.browser.servlet.InitServlet</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>com.browser.filter.EncodingFilter</filter-class>
    <init-param>
      <param-name>Encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>loginFilter</filter-name>
    <filter-class>com.browser.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loginFilter</filter-name>
    <url-pattern>/main.html</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>loginFilter</filter-name>
    <url-pattern>/view/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>loginFilter</filter-name>
    <url-pattern>/json/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>jsonFilter</filter-name>
    <filter-class>com.browser.filter.JSONFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>jsonFilter</filter-name>
    <url-pattern>/json/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.browser.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logoutServlet</servlet-name>
    <servlet-class>com.browser.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logoutServlet</servlet-name>
    <url-pattern>/json/logout.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>menuServlet</servlet-name>
    <servlet-class>com.browser.servlet.MenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>menuServlet</servlet-name>
    <url-pattern>/json/menu.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>roleServlet</servlet-name>
    <servlet-class>com.browser.servlet.RoleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>roleServlet</servlet-name>
    <url-pattern>/json/role.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userServlet</servlet-name>
    <servlet-class>com.browser.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userServlet</servlet-name>
    <url-pattern>/json/user.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>departmentServlet</servlet-name>
    <servlet-class>com.browser.servlet.DepartmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>departmentServlet</servlet-name>
    <url-pattern>/json/department.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>lineServlet</servlet-name>
    <servlet-class>com.browser.servlet.LineServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>lineServlet</servlet-name>
    <url-pattern>/json/line.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fenceServlet</servlet-name>
    <servlet-class>com.browser.servlet.FenceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fenceServlet</servlet-name>
    <url-pattern>/json/fence.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>vehicleServlet</servlet-name>
    <servlet-class>com.browser.servlet.VehicleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>vehicleServlet</servlet-name>
    <url-pattern>/json/vehicle.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>schemeServlet</servlet-name>
    <servlet-class>com.browser.servlet.SchemeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>schemeServlet</servlet-name>
    <url-pattern>/json/scheme.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>runServlet</servlet-name>
    <servlet-class>com.browser.servlet.RunServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>runServlet</servlet-name>
    <url-pattern>/json/run.do</url-pattern>
  </servlet-mapping>
</web-app>